        <activity
            android:name=".Actividad"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AgregarDato"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Consulta"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Menu"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Informacion"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Preguntas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Status"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Logros"
            android:screenOrientation="portrait" />
        <activity android:name=".Grafica" />
        <activity android:name=".CuestDiab" />
        <activity android:name=".CuestiPresion"></activity>
    </application>

</manifest>